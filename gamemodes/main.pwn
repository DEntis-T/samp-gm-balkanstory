if(i != INVALID_VEHICLE_ID) {

	if(Parked[i] != -1) {

		new Float:Pos[3];
		GetVehiclePos(i, Pos[0], Pos[1], Pos[2]);

		new parkID = IsCordsInOtherParkingZone(Pos[0], Pos[1]);

		if(parkID != -1) {
			ParkingPlaces[parkID][Parked[i]][parkTaken] = false;
		}
	}

	pizzaVehicle[i] = false;
	farmerVehicles[i][fvSeeds] = 0;
