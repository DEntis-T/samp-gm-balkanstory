CMD:daj(playerid, params[]) {
new x_nr[32],
	giveplayerid;

if(sscanf(params, "s[32] ", x_nr)) {
	SendUsageMessage(playerid, "/daj [Opcija] [ID/Deo Imena] [kolicina]");
