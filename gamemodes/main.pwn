			EnableAntiCheatForPlayer(playerid, 49, 1);
		}
	}
	else {
		atmTDControl(playerid, false);

		Fusion_KillTimer(atmrobTimer[playerid]);

		PostaviWanted(playerid, "Pokusaj pljacke bankomata", "Nepoznati", 3);
