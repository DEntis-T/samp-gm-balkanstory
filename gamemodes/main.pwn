	TazerMetkovi[playerid] = ammo;
	antiCheatCS[playerid][WeaponDetect] = 3;
	RemovePlayerWeapon(playerid, 23);
	GiveWeaponToPlayer(playerid, lastweapon[playerid][0], lastweapon[playerid][1]);
