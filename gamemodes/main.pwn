	lastweapon[playerid][1] = ammo;
	GiveWeaponToPlayer(playerid, 23, TazerMetkovi[playerid]);
	ImaTazer[playerid] = true;
	SCM(playerid, BELA, "Izvadio si tazer iz futrole.");}
else if(ImaTazer[playerid] == true) {
