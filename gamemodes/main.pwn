GiveWeaponToPlayer(playerid, weaponid, ammo) {

	if(antiCheatCS[playerid][WeaponCheck] == 1) antiCheatCS[playerid][WeaponCheck] = 2;
if(IsValidWeapon(weaponid)) {

	antiCheatCS[playerid][LastAmmoChecks] = 0;
