		if(igrac != INVALID_PLAYER_ID) {
		if(IsPlayerNearPlayer(3.0, playerid, igrac)) {
			if(IsPlayerInAnyVehicle(igrac)) return SendErrorMessage(playerid, "Ne mozes se rukovati sa osobom koja je u vozilu!");
